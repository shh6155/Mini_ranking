<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel='stylesheet' href='/stylesheets/ranking.css' >
    <link href="https://fonts.googleapis.com/css?family=Black+Han+Sans:400" rel="stylesheet"> 
  </head>

  <body id="bodycolor"   onload ="refresh_timer()" >
    <div id="rankingbox">
      <div id="title"><%= title %></div>
      <div id="time"></div>
      <%
        rank.forEach(function(item,index){
      %>
        <div id="name">
            <%= item.name %>
              <br>&emsp;&emsp;&ensp;
            <%= item.user %>
              <br>
        </div>
      <%
        });
      %>
      <br>
    </div>

    <script  src='/javascripts/ranking.js' type="text/javascript"></script>  
  </body>
</html>

